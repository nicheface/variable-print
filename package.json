{
  "publisher": "nicheface",
  "name": "variable-print",
  "displayName": "variable-print",
  "description": "print selected variable js,rust,py,java and so on",
  "version": "1.0.2",
  "repository": "https://github.com/nicheface/variable-print.git",
  "license": "GPL",
  "icon": "imags/logo.png",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.printVariable",
        "title": "Print Selected Variable"
      }
    ],
    "keybindings": [
      {
          "command": "extension.printVariable",
          "key": "ctrl+alt+p",
          "mac": "ctrl+alt+p",
          "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "variable-print",
      "properties": {
          "variable-print.customPrintStatements": {
              "type": "object",
              "default": {
                  "rust": "println!(\"row: $row - col: $col $v -> {:?}\", &$v);",
                  "go": "fmt.Printf(\"row: $row - col: $col $v -> %#v\n\",$v)",
                  "h|c|cpp":"printf(\"row: $row - col: $col $v -> %s\n\", $v);",
                  "js":"console.log($v);",
                  "py":"print(f\"row: $row - col: $col $v -> {$v}\")",
                  "java":"System.out.println($v);",
                  "rb":"puts \"row: $row - col: $col $v -> #{$v}\"",
                  "php":"var_dump($v);",
                  "lua":"print(\"row: $row - col: $col $v ->(\",type($v),\")\",$v)",
                  "kt":"println(\"row: $row - col: $col $v -> \"+$v)",
                  "cs":"Console.WriteLine(\"row: $row - col: $col $v -> {0}\", $v);",
                  "swift":"print(\"row: $row - col: $col $v -> \\($v)\")",
                  "dart":"print(\"row: $row - col: $col $$v -> $v ${$$v.runtimeType}\");",
                  "pl":"say \"row: $row - col: $col $v -> $$v\";",
                  "scala":"println(s\"row: $row - col: $col $v -> ${$v}\")",
                  "hrl|erl":"io:format(\"~w\", [$v]),",
                  "jl":"println(\"row: $row - col: $col $v ->\", $v)",
                  "hs":"putStrLn(\"row: $row - col: $col $v ->\" ++ $v)",
                  "zig":"std.debug.print(\"row: $row - col: $col $v -> {}\n\", .{$v});",
                  "v":"print($v)"
                
              }
          }
      }
    }
  },

  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "@vscode/test-electron": "^2.3.4"
  }
}
